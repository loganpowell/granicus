{
    "name": "amp-granicus",
    "author": "Logan Powell",
    "license": "MIT",
    "version": "0.2.67",
    "description": "ETL chron job between Granicus API and GovCloud",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "repository": "https://github.com/loganpowell/amp-granicus",
    "homepage": "https://github.com/loganpowell/amp-granicus",
    "scripts": {
        "pages": "gh-pages -d @-0/spool",
        "clean": "rm -rf ./lib",
        "typewatch": "tsc --project tsconfig.json",
        "types": "tsc --project tsconfig.build.json",
        "madge": "madge --circular lib/",
        "tests": "npm run clean && npm run types && npm run madge && concurrently \"npm run typewatch\" \"jest --watchAll\" ",
        "jsdoc": "jsdoc -c jsdoc/jsdoc.json --verbose --debug",
        "docs": "npm run types && npm run jsdoc",
        "patch": "npm version patch && npm run docs && npm run pages && npm publish",
        "mockapi": "amplify mock api",
        "postgit": "git push origin master && npm run patch",
        "ncu": "ncu -u && npm i && npm audit fix",
        "git": "npm run ncu && git add . && git commit -m",
        "start": "node lib/index.js",
        "build": "npm run types && parcel build lib/index.js --target node --bundle-node-modules",
        "build:upload": "npm run types && parcel build lib/upload/index.js --out-dir upload --target node --bundle-node-modules",
        "c:1:b": "node scripts/build job=1",
        "c:2:b": "node scripts/build job=2",
        "c:3:b": "node scripts/build job=3",
        "c:4:b": "node scripts/build job=4",
        "c:5:b": "node scripts/build job=5",
        "c:1:p": "node scripts/prime job=1",
        "c:2:p": "node scripts/prime job=2",
        "c:3:p": "node scripts/prime job=3",
        "c:4:p": "node scripts/prime job=4",
        "c:5:p": "node scripts/prime job=5",
        "cron:all:build": "npm run build && npm run c:1:b && npm run c:2:b && npm run c:3:b && npm run c:4:b && npm run c:5:b",
        "cron:all:prime": "npm run c:1:p && npm run c:2:p && npm run c:3:p && npm run c:4:p && npm run c:5:p",
        "upload:build": "npm run build:upload && rm -rf amplify/backend/function/upload/src/upload && cp -a upload amplify/backend/function/upload/src/upload",
        "push": "amplify push"
    },
    "keywords": [
        "ETL",
        "GQL"
    ],
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=12"
    },
    "dependencies": {
        "@-0/spool": "^0.2.78",
        "@-0/utils": "^0.2.66",
        "@thi.ng/arrays": "^0.6.5",
        "@thi.ng/associative": "4.0.6",
        "@thi.ng/atom": "^4.1.5",
        "@thi.ng/checks": "^2.6.4",
        "@thi.ng/csp": "^1.1.21",
        "@thi.ng/paths": "^4.0.4",
        "@thi.ng/rstream": "^4.0.7",
        "@thi.ng/transducers": "^6.4.8",
        "async-sema": "^3.1.0",
        "aws-sdk": "^2.801.0",
        "dayjs": "^1.8.26",
        "dotenv": "^8.2.0",
        "esm": "^3.2.25",
        "json-to-graphql-query": "^2.0.0",
        "nanographql": "^2.0.0",
        "node-fetch": "^2.6.0",
        "object.fromentries": "^2.0.2"
    },
    "peerDependencies": {},
    "devDependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.9.6",
        "@types/jest": "^25.2.1",
        "@types/node": "^13.13.4",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "babel-jest": "^25.5.1",
        "better-docs": "^2.0.0",
        "cross-env": "^7.0.2",
        "eslint": "^6.8.0",
        "gh-pages": "^2.2.0",
        "jest": "^25.5.3",
        "json2csv": "^5.0.3",
        "prettier": "^2.1.2",
        "ts-jest": "^25.4.0"
    },
    "prettier": {
        "singleQuote": false,
        "printWidth": 100,
        "endOfLine": "lf",
        "tabWidth": 4,
        "semi": false,
        "arrowParens": "avoid",
        "bracketSpacing": true
    }
}
